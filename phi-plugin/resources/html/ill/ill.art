{{extend defaultLayout}}
{{block 'css'}}
    <link rel="stylesheet" href="{{_res_path}}html/ill/ill.css">
{{/block}}

{{block 'main'}}
    <img src="{{illustration}}" alt="{{illustration}}">
    <div class="info-box">
        <div class="phi">
            <img src="{{_res_path}}html/otherimg/title.png" alt="Phigros">
        </div>
        <div class="line"></div>
        <div class="info">
            <div class="up">
                <p>Illustrator</p>
            </div>
            <div class="down">
                <p>{{illustrator}}</p>
            </div>
        </div>
    </div>
{{/block}}
